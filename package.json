{
  "name": "tech-pages",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint": "^8.29.0",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@3.3.0",
  "dependencies": {
    "@mdx-js/loader": "next",
    "@mdx-js/react": "^2.1.5",
    "@next/mdx": "^13.0.6",
    "@reach/disclosure": "^0.18.0",
    "@reach/tooltip": "^0.18.0",
    "gray-matter": "^4.0.3",
    "rehype-highlight": "^6.0.0",
    "remark-frontmatter": "^4.0.1"
  }
}
